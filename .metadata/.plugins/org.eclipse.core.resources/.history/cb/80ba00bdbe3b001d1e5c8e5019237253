public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	
		System.out.println(CheckThreeDigit(13));
	
	}
	
	public static boolean CheckThreeDigit(int n)
	{
		int ans=0;
		
		while(n!=0)
		{
			n=n/10;
			ans++;
		}
		
		return ans==3?true:false;
		
	}

}
